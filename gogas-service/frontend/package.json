  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build  && ./buildManager.sh",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write **/*.js",
    "lint": "eslint --ignore-pattern src/serviceWorker.js src/**.js ",
    "lint:fix": "eslint --ignore-pattern src/serviceWorker.js --fix src/**.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "post-merge-install",
      "post-rebase": "post-merge-install"
    }
  }
}
